{
  "hosting": {
    "public": "./",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
      "source": "**/*.@(jpg|jpeg|gif|png|mp3|js|css)",
      "headers": [ {
        "key": "Cache-Control",
        "value": "max-age=21600"
      }]
    }]
  }
}
